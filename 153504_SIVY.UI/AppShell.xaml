<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="_153504_SIVY.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:_153504_SIVY.UI"
    xmlns:pages="clr-namespace:_153504_SIVY.UI.Pages">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"
        Icon="home_img.png"/>
    
    <ShellContent
        Title="Performers list"
        ContentTemplate="{DataTemplate pages:Performers}"
        Route="Performers"
        Icon="performer_img.png"/>


</Shell>
